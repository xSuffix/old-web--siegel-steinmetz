{
  "connection": "default",
  "collectionName": "pages",
  "info": {
    "name": "Page"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "enabled": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "name": {
      "type": "string"
    },
    "widgets": {
      "collection": "widget"
    },
    "routes": {
      "via": "page",
      "collection": "route"
    }
  }
}
